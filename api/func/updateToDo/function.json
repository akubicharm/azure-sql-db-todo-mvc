{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "put"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "outputDocument",
      "databaseName": "todoDB",
      "collectionName": "todo",
      "createIfNotExists": "false",
      "connectionStringSetting": "AzureWebJobsStorage"
    }
  ]
}
